    "test-coverage": "jest --coverage --silent",
    "postinstall": "bower install",
    "prettier": "prettier-eslint '{lib,bin,config}/**/*.js' --write"
  },
  "dependencies": {
    "@yonomi/thincloud-node-sdk": "1.0.4",
    "aws-sdk": "2.224.1",
