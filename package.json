  },
  "engines": {
    "node": "6.10.x",
    "npm": "5.x.x"
  },
  "bin": {
    "thincloud-test-device": "./bin/thincloud-test-device"
  },
  "main": "lib/device.js",
  "scripts": {
    "codecov": "codecov",
    "clean": "rimraf -r node_modules",
    "lint": "eslint lib config bin",
