    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "prettier-eslint-cli": "4.7.1",
    "eslint-plugin-compat": "2.2.0",
    "jest": "20.0.4",
    "nsp": "2.6.3"
